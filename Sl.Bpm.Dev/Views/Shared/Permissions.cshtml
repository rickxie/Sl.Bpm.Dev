@using Sl.Bpm.Repository.Dto
@using MiniAbp
@{
    Layout = null;
    var permissions = ViewBag.Permissions as List<NameValue<string, bool>>;
    var user = ViewBag.User as string;
}
@*<script>*@
window.$currentUserPermissions = {
@if (permissions != null)
{
    for (int i = 0; i < permissions.Count; i++)
    {
        @(new HtmlString("'" + permissions[i].Name + "'" + ":" + (permissions[i].Value ? "true" : "false") + ","))
    }
}
    }

window.$currentUser = @(new HtmlString(user));
window.$errorUrl = '';
@*</script>*@