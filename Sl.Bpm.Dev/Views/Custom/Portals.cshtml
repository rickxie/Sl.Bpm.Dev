    .new-employee li .mess h3 { font-size: 14px; font-weight: bold }
    .new-employee li .mess h4 { font-size: 12px; padding-top: 10px; }
    .new-employee li .mess h5 { font-size: 12px; margin-top: 8px }
    .portals-foot2 { background: #666; color: #fff; padding: 20px; margin-top: 25px }
