@section foot
{
    <script src="~/Content/Lib/jquery-1.11.3/jquery-1.11.3.js"></script>
    <!-- Initialize Swiper -->
    <script>
        var swiper9 = new Swiper('.sc9', {
            direction: 'vertical',
            slidesPerView: 5,
            loop: true,
            autoplay: true
        });
        $(".sc9").hover(function () {
            swiper9.autoplay.stop();
        }, function () {
            swiper9.autoplay.start();
        });
    </script>
}
<style>
    .portals-title { font-size: 18px; color: #666; font-weight: bold; border-bottom: 2px solid #F1F1F1; line-height: 42px; margin-bottom: 16px; }
